<body>
    <!-- main -->
    <div class="main">
        <div class="main-info w3l">
            <div class="main-row">
                <!-- main-row-one -->
                <div class="profile-grid logo wthree">
                    <img src="~/Images/murat.png" style="width:133px;height:133px;" alt="">
                    <h2>Murat TOKAK</h2>
                    <p>Erzincan Binali Yıldırım Üniversitesi</p>
